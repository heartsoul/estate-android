<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_52"
    android:background="@color/c_6cc1de">

    <TextView
        android:id="@+id/quality_model_list_item_obj_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_20"
        android:ellipsize="end"
        android:layout_centerVertical="true"
        android:maxLines="1"
        android:textColor="@color/white"
        android:textSize="@dimen/dimen_16"
        />

    <RelativeLayout
        android:id="@+id/quality_model_list_item_obj_standard"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dimen_35"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@id/quality_model_list_item_obj_name">

        <ImageView
            android:layout_width="@dimen/dimen_15"
            android:layout_height="@dimen/dimen_15"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_16"
            android:layout_marginRight="@dimen/dimen_16"
            android:background="@drawable/icon_module_standard_white" />
    </RelativeLayout>


    <ImageView
        android:id="@+id/quality_model_list_item_obj_arrow"
        android:layout_width="@dimen/dimen_05"
        android:layout_height="@dimen/dimen_12"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/dimen_21"
        android:background="@drawable/icon_arrow_right_white" />

    <RelativeLayout
        android:id="@+id/quality_model_list_item_obj_create"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@id/quality_model_list_item_obj_arrow">

        <ImageView
            android:layout_width="@dimen/dimen_21"
            android:layout_height="@dimen/dimen_21"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_10"
            android:layout_marginRight="@dimen/dimen_25"
            android:layout_marginTop="@dimen/dimen_14"
            android:layout_marginBottom="@dimen/dimen_14"
            android:background="@drawable/icon_module_create_white" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_0.5"
        android:layout_alignParentBottom="true"
        android:background="@color/c_8ccccccc" />
</RelativeLayout>