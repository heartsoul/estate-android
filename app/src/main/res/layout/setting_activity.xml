<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <View
        android:id="@+id/setting_status"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_20"
        android:background="@color/c_00baf3" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_44"
        android:background="@color/c_00baf3">

        <RelativeLayout
            android:id="@+id/setting_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="@dimen/dimen_09"
                android:layout_height="@dimen/dimen_20"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen_20"
                android:layout_marginRight="@dimen/dimen_10"
                android:background="@drawable/icon_back_white" />
        </RelativeLayout>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="@string/str_set"
            android:textColor="@color/white"
            android:textSize="@dimen/dimen_17" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/c_f7f7f7"
            android:orientation="vertical">


            <!--修改密码-->
            <RelativeLayout
                android:id="@+id/setting_change_password"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_change_password_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_change_password" />

                <ImageView
                    android:layout_width="@dimen/dimen_05"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:background="@drawable/icon_arrow_right_gray" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_change_password_icon"
                    android:text="@string/str_set_change_psd"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_10"
                android:background="@color/c_f7f7f7" />

            <!--icon_flag_close wifi自动更新-->
            <RelativeLayout
                android:id="@+id/setting_wlan"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_wlan_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_version" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_wlan_icon"
                    android:text="@string/str_set_wlan"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />

                <ImageView
                    android:id="@+id/setting_wlan_flag"
                    android:layout_width="@dimen/dimen_40"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:background="@drawable/icon_flag_open" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_0.5"
                android:layout_marginLeft="@dimen/dimen_20"
                android:background="@color/c_f7f7f7" />
            <!--版本信息-->
            <RelativeLayout
                android:id="@+id/setting_version_info"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_version_info_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_version" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_version_info_icon"
                    android:text="@string/str_set_version_info"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />

                <ImageView
                    android:id="@+id/setting_version_info_arrow"
                    android:layout_width="@dimen/dimen_05"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:background="@drawable/icon_arrow_right_gray" />

                <TextView
                    android:id="@+id/setting_version_info_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:layout_toLeftOf="@id/setting_version_info_arrow"
                    android:text="@string/str_set_version"
                    android:textColor="@color/c_999992"
                    android:textSize="@dimen/dimen_14" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_0.5"
                android:layout_marginLeft="@dimen/dimen_20"
                android:background="@color/c_f7f7f7" />
            <!--意见反馈-->
            <RelativeLayout
                android:id="@+id/setting_feedback"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_feedback_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_feedback" />

                <ImageView
                    android:layout_width="@dimen/dimen_05"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:background="@drawable/icon_arrow_right_gray" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_feedback_icon"
                    android:text="@string/str_set_feedback_title"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_0.5"
                android:layout_marginLeft="@dimen/dimen_20"
                android:background="@color/c_f7f7f7" />
            <!--联系我们-->
            <RelativeLayout
                android:id="@+id/setting_contact_us"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_contact_us_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_contact_us" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_contact_us_icon"
                    android:text="@string/str_set_contact"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />

                <TextView
                    android:id="@+id/setting_contact_us_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:textColor="@color/c_00baf3"
                    android:textSize="@dimen/dimen_14" />

            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_0.5"
                android:layout_marginLeft="@dimen/dimen_20"
                android:background="@color/c_f7f7f7" />
            <!--关于我们-->
            <RelativeLayout
                android:id="@+id/setting_about_us"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_about_us_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_about_us" />

                <ImageView
                    android:layout_width="@dimen/dimen_05"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:background="@drawable/icon_arrow_right_gray" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_about_us_icon"
                    android:text="@string/str_set_about_us"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_10"
                android:background="@color/c_f7f7f7" />
            <!--切换项目-->
            <RelativeLayout
                android:id="@+id/setting_change_project"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_change_project_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_change_project" />

                <ImageView
                    android:layout_width="@dimen/dimen_05"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:background="@drawable/icon_arrow_right_gray" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_change_project_icon"
                    android:text="@string/str_set_change_project"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_10"
                android:background="@color/c_f7f7f7" />
            <!--离线设置-->
            <RelativeLayout
                android:id="@+id/setting_offline"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50"
                android:background="@color/white">

                <ImageView
                    android:id="@+id/setting_offline_icon"
                    android:layout_width="@dimen/dimen_24"
                    android:layout_height="@dimen/dimen_24"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:background="@drawable/icon_setting_offline" />

                <ImageView
                    android:layout_width="@dimen/dimen_05"
                    android:layout_height="@dimen/dimen_12"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="@dimen/dimen_20"
                    android:background="@drawable/icon_arrow_right_gray" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/dimen_20"
                    android:layout_toRightOf="@id/setting_offline_icon"
                    android:text="@string/str_set_offline"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen_14" />
            </RelativeLayout>

            <Button
                android:id="@+id/setting_signout_btn"
                android:layout_width="@dimen/dimen_297"
                android:layout_height="@dimen/dimen_40"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="@dimen/dimen_30"
                android:background="@drawable/corner_radius_33_blue_bg"
                android:text="@string/str_set_login_out"
                android:textColor="@color/white"
                android:textSize="@dimen/dimen_16" />
        </LinearLayout>
    </ScrollView>

</LinearLayout><!--<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"--><!--android:layout_width="match_parent"--><!--android:layout_height="match_parent"--><!--android:orientation="vertical">--><!--<ScrollView--><!--android:layout_width="match_parent"--><!--android:layout_height="match_parent"--><!--android:fillViewport="true">-->

